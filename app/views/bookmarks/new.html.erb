<h1>New bookmark</h1>
<%= simple_form_for [@list, @bookmark] do |f| %>
  <%= f.association :movie, prompt: 'Select a Movie' %>
  <%= f.input :comment, placeholder: 'Please enter a comment' %>
  <%= f.submit class: 'btn btn-flat btn-primary' %>
<% end %>
<br>
<p><%= link_to 'Back to all lists', lists_path %></p>
